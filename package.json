{
  "name": "post-creator",
  "version": "1.0.0",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=development nest start --watch",
    "start": "NODE_ENV=production node dist/main.js",
    "db:generate": "ts-node -r dotenv/config src/common/database/run-drizzle.ts generate",
    "db:push": "ts-node -r dotenv/config src/common/database/run-drizzle.ts push",
    "db:pull": "ts-node -r dotenv/config src/common/database/run-drizzle.ts pull",
    "db:studio": "ts-node -r dotenv/config src/common/database/run-drizzle.ts studio",
    "db:migrate": "ts-node -r dotenv/config src/common/database/run-drizzle.ts migrate"
  },
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "@aws-sdk/s3-request-presigner": "^3.850.0",
    "@fastify/cookie": "^11.0.2",
    "@fastify/multipart": "^9.0.3",
    "@google/generative-ai": "^0.24.1",
    "@nestjs/common": "^11.1.4",
    "@nestjs/core": "^11.1.4",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-fastify": "^11.1.4",
    "@types/module-alias": "^2.0.4",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3",
    "fastify": "^5.4.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "multer": "^2.0.1",
    "nestjs": "^0.0.1",
    "nestjs-pino": "^4.4.0",
    "pg": "^8.16.3",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.10",
    "@types/pg": "^8.15.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "_moduleAliases": {
    "@common": "dist/common",
    "@auth": "dist/domains/auth",
    "@gemini": "dist/domains/gemini",
    "@post": "dist/domains/post",
    "@user": "dist/domains/user"
  }
}
